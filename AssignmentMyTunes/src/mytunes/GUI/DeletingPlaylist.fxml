   <children>
      <ButtonBar layoutY="60.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="buttonClosefx" mnemonicParsing="false" onAction="#buttonDelete" style="-fx-background-color: #AFF2FF;" text="Delete" />
            <Button mnemonicParsing="false" onAction="#buttonCancel" style="-fx-background-color: #AFF2FF;" text="Cancel" />
        </buttons>
      </ButtonBar>
      <Label layoutX="10.0" layoutY="14.0" text="Do you want to delete this playlist?" />
   </children>
</AnchorPane>
